<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>

<!-- 失败界面 -->
<script type="text/html" id="tpl_page_error">
<div class="page">
    <div class="weui-msg">
        <div class="weui-msg__icon-area"><i class="weui-icon-warn weui-icon_msg"></i></div>
        <div class="weui-msg__text-area">
            <h2 class="weui-msg__title" ins="common.workflow.cmnMsg" inm="msgProcessErrorTitle"></h2>
            <p class="weui-msg__desc" id="processErrorMsg"></p>
        </div>
        <div class="weui-msg__opr-area">
            <p class="weui-btn-area">
                <a href="javascript:history.back();" class="weui-btn weui-btn_default" ins="common.workflow.cmnMsg" inm="btnErroBack"></a>
            </p>
        </div>
    </div>
</div>
</script>