<swiper indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item bindtap="click">
      <image id="{{index}}" src="{{item}}" class="slide-image" width="355" height="150"/>
    </swiper-item>
  </block>
</swiper>

<view wx:key="{{index}}" wx:for="{{types}}" class="mytype" bindtap="getMyaudio" id="{{item}}">{{item}}</view>